<Window x:Class="Project.RemoveWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project"
        mc:Ignorable="d"
        Title="RemoveWindow" Height="150" Width="800" Background="#FF98BCE0" FontSize="14">
    <Grid>
        <StackPanel Margin="150,15,150,5">
            <TextBlock Text="Name of item to remove:"/>
            <ComboBox x:Name="Name"/>
            <Button Content="Submit" Click="remove_Click" Margin="0,5,0,0"/>
        </StackPanel>
    </Grid>
</Window>
